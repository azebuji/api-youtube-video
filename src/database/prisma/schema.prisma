// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Login {
  uuid_login  String @id @default(uuid())
  name_login  String @unique @db.VarChar(60)
  name_person String

  @@map("login")
}
